# some more ls aliases
alias ll='ls -alFh'
alias la='ls -Ah'
alias l='ls -CFh'
alias L='ls -lFh'

alias dud='du --max-depth=0 --human-readable'
alias fd='find . -type d -name'
alias ff='find . -type f -name'

alias rm='rm -i'
alias mv='mv -i'
alias cp='cp -i'

alias -s zip='unzip -l'
alias -s rar='unrar l'
alias -s tar='tar -tvf'
alias -s tgz='tar -tzvf'
alias -s gz='tar -tzvf'
alias -s tbz='tar -tjvf'
alias -s bz2='tar -tjvf'
alias -s txz='tar -tJvf'
alias -s xz='tar -tJvf'
alias -s png='eog'
